{

  "manifest_version": 2,
  "name": "Better RCW/WAC",
  "version": "0.1",

  "description": "Reformats WAC and RCW on leg.wa.gov for easier reading.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.leg.wa.gov/*"],
      "js": ["rcwwac.js"],
      "css": ["spacing.css"]
    }
  ]

}
